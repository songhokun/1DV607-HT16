         <effect>
            <Glow />
         </effect>
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="english" layoutX="460.0" layoutY="102.0" text="English" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Glow />
         </effect>
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="swedish" layoutX="609.0" layoutY="102.0" text="Swedish" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Glow />
