            <Font size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Glow />
         </effect>
      </Label>
      <ChoiceBox fx:id="gameModeChoices" layoutX="460.0" layoutY="47.0" prefHeight="27.0" prefWidth="370.0">
