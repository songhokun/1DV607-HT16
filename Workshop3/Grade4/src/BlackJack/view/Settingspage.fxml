            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Glow />
         </effect>
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="language" layoutX="309.0" layoutY="101.0" text="Language" textFill="YELLOW">
