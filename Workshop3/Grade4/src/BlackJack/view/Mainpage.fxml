<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.*?>
<?import javafx.embed.swing.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BlackJack.controller.MainpageController">
   <children>
      <ImageView fx:id="imageView" fitHeight="700.0" fitWidth="850.0" pickOnBounds="true">
         <image>
            <Image url="@../../../../../../../../../../Downloads/unnamed.png" />
         </image></ImageView>
      <Label fx:id="play" layoutX="719.0" layoutY="572.0" onMouseClicked="#displayGamepage" text="PLAY" textFill="#bc1515">
         <font>
            <Font size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label fx:id="settings" layoutX="699.0" layoutY="610.0" onMouseClicked="#displaySettingspage" text="SETTINGS" textFill="#bc1515">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="quit" layoutX="730.0" layoutY="647.0"  text="QUIT" textFill="#bc1515">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
